{
  "name": "kittychan-domain",
  "version": "1.0.0",
  "description": "Domain microservice for kitty chan",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node ./dist/main.js",
    "dev": "nodemon src/main.ts",
    "build": "tsc",
    "build:kitty_chan_proto": "npx proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=./src/proto/ ./src/proto/kitty_chan.proto -y",
    "build:live_cord_proto": "npx proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=./src/proto/ ./src/proto/live_cord.proto -y",
    "build:proto": "cpx \"./src/proto/*.proto\" ./dist/proto",
    "lint": "eslint \"{src,apps,test}/**/*.ts\" --fix",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaga-live/kitty-chan-discord-ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaga-live/kitty-chan-discord-ts/issues"
  },
  "homepage": "https://github.com/jaga-live/kitty-chan-discord-ts#readme",
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "@live-apps/discord": "^1.0.10",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-handlebars": "^6.0.0",
    "@types/jest": "^29.2.5",
    "amqplib": "^0.10.3",
    "axios": "^1.1.3",
    "babel-jest": "^29.3.1",
    "bottleneck": "^2.19.5",
    "cors": "^2.8.5",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "express-handlebars": "^6.0.6",
    "google-protobuf": "^3.21.2",
    "grpc": "^1.24.11",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "ioredis": "^5.3.1",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.6.5",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.5.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.8.11",
    "@grpc/proto-loader": "^0.7.5",
    "@types/amqplib": "^0.10.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.12.4",
    "husky": "^8.0.3",
    "protobufjs": "^7.2.2"
  }
}
