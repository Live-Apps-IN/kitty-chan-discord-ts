{
  "name": "kittychan-domain",
  "version": "1.0.0",
  "description": "Domain microservice for kitty chan",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node ./dist/main.js",
    "dev": "nodemon src/main.ts",
    "build": "tsc",
    "build:kitty_chan_proto": "npx proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=./src/proto/ ./src/proto/kitty_chan.proto -y && npm run lint",
    "build:proto": "cpx \"./src/proto/*.proto\" ./dist/proto",
    "lint": "eslint \"{src,apps,test}/**/*.ts\" --fix",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/live-apps-in/kitty-chan-domain.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/live-apps-in/kitty-chan-domain/issues"
  },
  "homepage": "https://github.com/live-apps-in/kitty-chan-domain#readme",
  "dependencies": {
    "@babel/preset-env": "^7.23.9",
    "@elastic/elasticsearch": "^7.17.12",
    "@live-apps/discord": "^1.0.20",
    "amqplib": "^0.10.3",
    "axios": "^1.6.7",
    "babel-jest": "^29.7.0",
    "cors": "^2.8.5",
    "croner": "^8.0.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "google-protobuf": "^3.21.2",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "ioredis": "^5.3.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "reflect-metadata": "^0.2.1",
    "socket.io": "^4.7.4",
    "toad-scheduler": "^3.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "@types/amqplib": "^0.10.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.12.4",
    "husky": "^9.0.10",
    "prettier": "^3.2.4",
    "protobufjs": "^7.2.6"
  }
}
